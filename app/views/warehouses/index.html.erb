<h1>Warehouses</h1>
<%= link_to 'New Warehouse', new_warehouse_path, class: 'btn btn-primary' %>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Location</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @warehouses.each do |warehouse| %>
      <tr>
        <td><%= warehouse.name %></td>
        <td><%= warehouse.location %></td>
        <td>
          <%= link_to 'Edit', edit_warehouse_path(warehouse), class: 'btn btn-warning' %> |
          <%= link_to 'View', warehouse_path(warehouse), class: 'btn btn-warning' %> |
          <%= button_to 'Delete', warehouse, method: :delete, data: { confirm: 'Are you sure you want to delete this warehouse?' }, class: 'btn btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
